{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ng-ace": {
      "projectType": "library",
      "root": "projects/ng-ace",
      "sourceRoot": "projects/ng-ace/src",
      "prefix": "ng-ace",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/ng-ace/tsconfig.lib.json",
            "project": "projects/ng-ace/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ng-ace/src/test.ts",
            "tsConfig": "projects/ng-ace/tsconfig.spec.json",
            "karmaConfig": "projects/ng-ace/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ng-ace/tsconfig.lib.json",
              "projects/ng-ace/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "ng-ace-test": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/ng-ace-test",
      "sourceRoot": "projects/ng-ace-test/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "outputPath": "dist/ng-ace-test",
            "index": "projects/ng-ace-test/src/index.html",
            "main": "projects/ng-ace-test/src/main.ts",
            "polyfills": "projects/ng-ace-test/src/polyfills.ts",
            "tsConfig": "projects/ng-ace-test/tsconfig.app.json",
            "aot": false,
            "assets": [
              "projects/ng-ace-test/src/favicon.ico",
              "projects/ng-ace-test/src/assets"
            ],
            "styles": [
              "projects/ng-ace-test/src/styles.css"
            ],
            "scripts": [],
            "customWebpackConfig": {
              "path": "./webpack.config.js"
            }
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/ng-ace-test/src/environments/environment.ts",
                  "with": "projects/ng-ace-test/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-builders/custom-webpack:dev-server",
          "options": {
            "browserTarget": "ng-ace-test:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ng-ace-test:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ng-ace-test:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ng-ace-test/src/test.ts",
            "polyfills": "projects/ng-ace-test/src/polyfills.ts",
            "tsConfig": "projects/ng-ace-test/tsconfig.spec.json",
            "karmaConfig": "projects/ng-ace-test/karma.conf.js",
            "assets": [
              "projects/ng-ace-test/src/favicon.ico",
              "projects/ng-ace-test/src/assets"
            ],
            "styles": [
              "projects/ng-ace-test/src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ng-ace-test/tsconfig.app.json",
              "projects/ng-ace-test/tsconfig.spec.json",
              "projects/ng-ace-test/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/ng-ace-test/e2e/protractor.conf.js",
            "devServerTarget": "ng-ace-test:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ng-ace-test:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "ng-ace-test"
}