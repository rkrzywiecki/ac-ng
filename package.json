{
  "name": "ng-ace",
  "version": "0.0.0",
  "scripts": {
    "build": "npm run clean && npm run build:api && npm run build:lib && npm run build:schematics",
    "build:api": "node build-api.js",
    "build:lib": "ng build ng-ace",
    "build:schematics": "tsc -p projects/ng-ace/tsconfig.schematics.json",
    "copy:schemas": "cpx projects/ng-ace/schematics/*/schema.json dist/ng-ace/schematics",
    "copy:files": "cpx projects/ng-ace/schematics/*/files/** dist/ng-ace/schematics",
    "copy:collection": "cpx projects/ng-ace/schematics/collection.json dist/ng-ace/schematics",
    "postbuild": "npm run copy:schemas && npm run copy:files && npm run copy:collection",
    "clean": "rimraf dist",
    "unadd": "rimraf atlassian-connect.json && rimraf webpack.config.js && rimraf config.json && rimraf credentials.json",
    "dry-run:add": "npm run unadd && npm run build && npm link dist/ng-ace && ng add ng-ace"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.0.0",
    "@angular/common": "~8.0.0",
    "@angular/compiler": "~8.0.0",
    "@angular/core": "~8.0.0",
    "@angular/forms": "~8.0.0",
    "@angular/platform-browser": "~8.0.0",
    "@angular/platform-browser-dynamic": "~8.0.0",
    "@angular/router": "~8.0.0",
    "jsdom": "^15.1.1",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "xmldom": "^0.1.27",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^8.1.0",
    "@angular-devkit/build-angular": "~0.800.6",
    "@angular-devkit/build-ng-packagr": "~0.800.6",
    "@angular/cli": "~8.0.1",
    "@angular/common": "^8.0.0",
    "@angular/compiler-cli": "~8.0.0",
    "@angular/core": "^8.0.0",
    "@angular/language-service": "~8.0.0",
    "@atlaskit/css-reset": "^5.0.5",
    "@atlaskit/reduced-ui-pack": "^12.0.3",
    "@openapitools/openapi-generator-cli": "0.0.16-4.0.3",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/jsdom": "^12.2.4",
    "@types/node": "~8.9.4",
    "@types/xmldom": "^0.1.29",
    "atlassian-connect-express": "^3.5.1",
    "body-parser": "^1.19.0",
    "codelyzer": "^5.0.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "cpx": "^1.5.0",
    "errorhandler": "^1.5.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^5.1.0",
    "protractor": "~5.4.0",
    "rimraf": "^2.6.3",
    "sequelize": "^5.12.2",
    "sqlite3": "^4.0.9",
    "ts-node": "~7.0.0",
    "tsickle": "^0.35.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3"
  }
}
